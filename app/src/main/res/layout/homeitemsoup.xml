<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/roundcornorhomediary">


    <ImageView
        android:id="@+id/homesvlogbkg"
        android:layout_width="match_parent"
        android:layout_height="260dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <ImageView
        android:background="@drawable/homevlogroundcornorblack"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/homesvlogbkg"
        app:layout_constraintEnd_toEndOf="@id/homesvlogbkg"
        app:layout_constraintTop_toTopOf="@id/homesvlogbkg"
        app:layout_constraintBottom_toBottomOf="@id/homesvlogbkg"
        />


    <ImageView
        android:id="@+id/topgreen"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:scaleType="fitStart"
        android:src="@mipmap/soup_vlog_lable"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />






    <ImageView
        android:padding="0dp"
        android:id="@+id/homevloggif"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:src="@mipmap/main_item_live"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="@id/homesvlogbkg"
        app:layout_constraintEnd_toEndOf="@id/homesvlogbkg"
        app:layout_constraintTop_toBottomOf="@id/topgreen"
        app:layout_constraintBottom_toBottomOf="parent"

        />




    <TextView
        android:id="@+id/homesouptextmid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="我要加入\n我"
        android:textColor="#FFFFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/homevloggif"
        android:layout_marginBottom="10dp"
        android:layout_marginHorizontal="15dp"
        android:gravity="left"
        android:textSize="14sp"
        />








    <ImageView
        android:id="@+id/homedakahead"
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="15dp"
        android:src="@mipmap/head"
        android:layout_marginStart="12dp"/>

    <TextView
        android:id="@+id/homedakaneckname"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/homedakahead"
        app:layout_constraintBottom_toBottomOf="@id/homedakahead"
        app:layout_constraintStart_toEndOf="@id/homedakahead"
        app:layout_constraintEnd_toEndOf="parent"
        android:gravity="center|left"
        android:layout_marginLeft="5dp"
        android:textSize="10sp"
        android:text="5614654"
        android:textColor="@color/white"
        />








</androidx.constraintlayout.widget.ConstraintLayout>